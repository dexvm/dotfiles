{
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "datasourceTemplate": "git-refs",
      "fileMatch": [
        "^\\.chezmoiexternal\\.toml$"
      ],
      "matchStrings": [
        "# renovate: currentValue=(?<currentValue>.*)\\n.*(?<depName>https:\\/\\/github\\.com\\/\\w+\\/\\w+)\\/archive\\/(?<currentDigest>\\w+)\\.zip"
      ]
    }
  ]
}